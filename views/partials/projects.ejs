                <div class="card" style="width: 18rem;">
                    <img class="card-img-top" src="/images/<%=project.images%>" alt="Card image cap">
                    <div class="card-body d-flex justify-content-center">
                        <h5 class="card-title pt-2"><%=project.title%></h5>
                    </div>
                </div>
                <% } %>

            </div>
        </div>
    </div>


</div>

